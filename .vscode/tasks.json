{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "jeff",
      "command": "/usr/bin/clang++",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-Iinclude/",
        "-lpthread",
        "-std=c++14"
